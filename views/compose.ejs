<%- include("partials/header"); -%>
 
<form class="item" action="/compose" method="post">

    <h1>Compose</h1> 

    <div class="form-group">
        <label for="newTitle">Title</label>
        <input type="text" class="form-control" id="newTitle" name="postTitle" autocomplete="off">
        <label for="newPost">Post</label>
        <textarea class="form-control" name="postBody" id="newPost" rows="5" cols="30"></textarea>
    </div>
    <button class="btn btn-primary btn-md active" type="submit" name="button">Publish</button>

</form>

<%- include("partials/footer"); -%>